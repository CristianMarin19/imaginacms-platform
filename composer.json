{
    "name": "imagina/cms-platform",
    "description": "The AsgardCms application.",
    "keywords": [
        "cms",
        "asgardcms",
        "multilingual",
        "laravel",
        "laravel5"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/imagina/laravel-menus.git"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "astrotomic/laravel-translatable": "^11.9",
        "cartalyst/sentinel": "^4.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "imagina/laravel-menus": "dev-master",
        "imagina/stylist": "^2.0",
        "intervention/image": "^2.5",
        "laracasts/presenter": "^0.2.4",
        "laravel/framework": "^7.29",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^2.4",
        "maatwebsite/laravel-sidebar": "^2.4",
        "mcamara/laravel-localization": "^1.6",
        "nwidart/laravel-modules": "^6.0",
        "symfony/yaml": "^5.1",
        "tightenco/ziggy": "^0.9.4",
        "typicms/nestablecollection": "^1.1",
        "venturecraft/revisionable": "^1.35",
        "laravel/passport": "^9.3",

        "imagina/core-module": "4.0.x-dev",
        "imagina/dashboard-module": "4.0.x-dev",

        "imagina/tag-module": "4.0.x-dev",
        "imagina/media-module": "4.0.x-dev",
        "imagina/page-module": "4.0.x-dev",
        "imagina/menu-module": "4.0.x-dev",
        "imagina/setting-module": "4.0.x-dev",
        "imagina/translation-module": "4.0.x-dev",
        "imagina/workshop-module": "4.0.x-dev",
        "imagina/user-module": "4.0.x-dev",

        "imagina/iblog-module": "4.0.x-dev",
        "imagina/iforms-module": "4.0.x-dev",
        "imagina/notification-module": "4.0.x-dev",
        "imagina/slider-module": "4.0.x-dev",
        "imagina/isearch-module": "4.0.x-dev",
        "imagina/ibanners-module": "4.0.x-dev",
        "imagina/iprofile-module": "4.0.x-dev",
        "imagina/isite-module": "4.0.x-dev",
        "imagina/ilocations-module": "4.0.x-dev",
        "imagina/iredirect-module": "4.0.x-dev",

        "imagina/flatly-theme": "4.0.x-dev"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fzaninotto/faker": "^1.9.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "orchestra/testbench": "^5.7",
        "phpunit/phpunit": "^8.5",
        "predis/predis": "^1.1",
        "roave/security-advisories": "dev-master",
        "facade/ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        },
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "ln -s ../Themes/Adminlte/iadmin/ public/iadmin",
            "cp -Rf ./Themes/Flatly ./Themes/ImaginaTheme"
        ],
        "pre-update-cmd": [],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
